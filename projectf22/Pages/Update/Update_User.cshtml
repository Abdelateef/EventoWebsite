@page
@model projectf22.Pages.Update.Update_UserModel
@{
    <form method="post">
        <input type="text" asp-for="@Model.User.UserName" placeholder="Name" value="@Model.User.UserName" />
        <input type="text" asp-for="@Model.User.UserEmail" placeholder="Name" value="@Model.User.UserEmail" />
        <input type="text" asp-for="@Model.User.UserPassword" placeholder="Name" value="@Model.User.UserPassword" />
        <br />
        <button class="btn btn-primary" type="submit">Update User</button>
    </form>
}

